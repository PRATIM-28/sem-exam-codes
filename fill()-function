#include<iostream>
using namespace std;

int main()

{
    cout.fill('<');
    for(int i=1;i<=5;i++)
    {
        cout.width(5);
        cout<<i;
        
        cout.width(10);
        cout<<(i+1);
        cout<<"\n";
        
        if(i==3)
        {
        cout.fill('>');
        }
        
    }
    return 0;
}
