#include<iostream>
#include<cmath>

using namespace std;

int main()
{
    cout<<"SETTING PRECISION TO 3\n";
    cout.precision(3);
    
    cout.width(10);
    cout<<"VALUES";
    
    cout.width(20);
    cout<<"SQUARE_ROOT_VALUES";
    
    cout<<endl;
    
    for(int i=0;i<5;i++)
    {
        cout.width(8);
        cout<<i;
        
        cout.width(14);
        cout<<sqrt(i);
        
        cout<<"\n";
    }
}
